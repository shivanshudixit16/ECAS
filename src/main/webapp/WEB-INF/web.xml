<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>AdminFilter</filter-name>
  	<display-name>AdminFilter</display-name>
  	<description></description>
  	<filter-class>Filters.AdminFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>TeacherFilter</filter-name>
  	<display-name>TeacherFilter</display-name>
  	<description></description>
  	<filter-class>Filters.TeacherFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>AdminFilter</filter-name>
  	<url-pattern>/teacher_reg.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>AdminFilter</filter-name>
  	<url-pattern>/admin_home.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>TeacherFilter</filter-name>
  	<url-pattern>/generate_rolls.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>TeacherFilter</filter-name>
  	<url-pattern>/student_reg.jsp</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>TeacherFilter</filter-name>
  	<url-pattern>/teacher_home.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>teachersubmit</servlet-name>
    <servlet-class>TeacherSubmit</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>studentsubmit</servlet-name>
    <servlet-class>StudentSubmit</servlet-class>
  </servlet>
   <servlet>
   	<servlet-name>ImageSend</servlet-name>
   	<display-name>ImageSend</display-name>
   	<description></description>
   	<servlet-class>imagehandler.ImageSend</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>GenerateRolls</servlet-name>
   	<display-name>GenerateRolls</display-name>
   	<description></description>
   	<servlet-class>general.GenerateRolls</servlet-class>
   </servlet>
   <servlet>
   	<servlet-name>GetAdmit</servlet-name>
   	<display-name>GetAdmit</display-name>
   	<description></description>
   	<servlet-class>general.GetAdmit</servlet-class>
   </servlet>
   <servlet-mapping>
    <servlet-name>teachersubmit</servlet-name>
    <url-pattern>/teacher_regsubmit</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>studentsubmit</servlet-name>
    <url-pattern>/student_regsubmit</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>ImageSend</servlet-name>
   	<url-pattern>/ImageSend</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>GenerateRolls</servlet-name>
   	<url-pattern>/genroll</url-pattern>
   </servlet-mapping>
   <servlet-mapping>
   	<servlet-name>GetAdmit</servlet-name>
   	<url-pattern>/getadmit</url-pattern>
   </servlet-mapping>
</web-app>
